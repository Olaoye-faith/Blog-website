<%- include ("partials/header") -%>
<h3>COMPOSE</h2>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="inputtext">Title</label>
        <input class="form-control" type="text" name="posttitle">
    </div>
    
    <div class="form-group">
        <label for="textarea">Post</label>
        <textarea class="form-control" name="postbody" cols="25" rows="5"></textarea>
    </div>

    <button type="submit" class="btn btn-primary" name="submit">Publish</button>
</form>
